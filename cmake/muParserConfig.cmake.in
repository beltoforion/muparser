# - Config file for the MUPARSER package
# It defines the following variables
#  MUPARSER_INCLUDE_DIRS - include directories for MUPARSER
#  MUPARSER_LIBRARY_DIRS - library directories for MUPARSER (normally not used!)
#  MUPARSER_LIBRARIES    - libraries to link against
#  MUPARSER_DEFINITIONS  - List of compilation flags -DTOTO to export

# defined since 2.8.3
if(CMAKE_VERSION VERSION_LESS 2.8.3)
  get_filename_component (CMAKE_CURRENT_LIST_DIR ${CMAKE_CURRENT_LIST_FILE} PATH)
endif ()

# Tell the user project where to find our headers and libraries

set (MUPARSER_INCLUDE_DIRS "${CMAKE_CURRENT_LIST_DIR}/@MUPARSER_INCLUDE_DIR@")
set (MUPARSER_LIBRARY_DIRS "${CMAKE_CURRENT_LIST_DIR}/@MUPARSER_LIB_DIR@")

# Allows loading MUPARSER settings from another project
set (MUPARSER_CONFIG_FILE "${CMAKE_CURRENT_LIST_FILE}")

# List of compilation flags -DTOTO to export
set (MUPARSER_DEFINITIONS "@MUPARSER_DEFINITIONS@")

# Our library dependencies (contains definitions for IMPORTED targets)
include ("${CMAKE_CURRENT_LIST_DIR}/muParser-Targets.cmake")

# These are IMPORTED targets created by muParser-Targets.cmake
set (MUPARSER_LIBRARIES "muparser")

if(CMAKE_VERSION VERSION_LESS 2.8.3)
  set(CMAKE_CURRENT_LIST_DIR)
endif ()
